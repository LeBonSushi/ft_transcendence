{
  "name": "@transv2/shared",
  "version": "1.0.0",
  "description": "Shared code for TRANSv2 microservices",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
